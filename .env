#chromedriver path
local_chromedriver_path = './chromedriver.exe'
aws_chromedriver_path = 'none'
pi_chromedriver_path = '/usr/bin/chromedriver'
##heroku supplies the env var

#motd website
motd_url = '*'
#corona beer image url
corona_image_url = 'https://github.com/jackowski626/didiBot/blob/master/images/corona.png?raw=true'
