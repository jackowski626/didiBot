#Example .env file

#Credentials

#Discord token
TOKEN = '*'

#Remote FTP json db
remote_db_ftp_host = '*'
remote_db_ftp_user = '*'
remote_db_ftp_pw = '*'
remote_db_ftp_path = '*'

#Reddit
reddit_client_id = '*'
reddit_client_secret = '*'
reddit_user_agent = '*'
reddit_user_agent_requests = '*'

#Rapid API
rapid_api_key = '*'

#Riot API
riot_api_key = '*'


#Other

#db file names
local_db_filename = './dididb.json'
